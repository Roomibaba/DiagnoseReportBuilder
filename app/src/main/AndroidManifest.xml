<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.diagnosereportbuilder">

    <application
        android:allowBackup="true"
        android:icon="@drawable/patient"
        android:label="@string/app_name"
        android:roundIcon="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.DiagnoseReportBuilder">
        <activity
            android:name=".Patient_Detail"
            android:exported="true" />
        <activity
            android:name=".Show_Patient_List"
            android:exported="true" />
        <activity
            android:name=".splashscreen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Initial_Diagnose"
            android:exported="true" />
        <activity
            android:name=".Vitals"
            android:exported="true" />
        <activity
            android:name=".Symptoms"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true" />
        <activity
            android:name=".Diagnose_Report"
            android:exported="true" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>